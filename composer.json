{
    "name": "kemper/elastic",
    "description": "Simple extension to allow for elasticsearch",
    "type": "bolt-extension",
    "keywords": [
        "elasticsearch",
        "search"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kemper Web Team",
            "email": "webmaster@kcpag.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kemper\\Elastic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kemper\\Elastic\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Kemper\\Elastic\\ElasticExtension",
        "bolt-icon": "elastic-logo.png"
    }
}
